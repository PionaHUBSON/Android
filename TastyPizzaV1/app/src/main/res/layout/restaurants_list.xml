<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ly"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="0dp">


        <TextView
            android:id="@+id/id_res"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/logo_img_res"
            android:layout_marginEnd="98dp"
            android:layout_marginBottom="-4dp"
            android:layout_toStartOf="@+id/name_res" />

        <TextView
            android:id="@+id/phone_number_res"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/divider"
            android:layout_marginTop="107dp"
            android:layout_marginEnd="11dp"
            android:layout_toStartOf="@+id/divider" />

        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="125dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="8dp"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="8dp"
            android:background="@drawable/selector2"
            android:foregroundGravity="center_horizontal"
            app:layout_constraintEnd_toEndOf="@+id/textView3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/view2"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <View
            android:id="@+id/divider"
            android:layout_width="2dp"
            android:layout_height="135dp"
            android:layout_alignTop="@+id/view3"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="0dp"
            android:layout_marginTop="-6dp"
            android:layout_marginEnd="270dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="@+id/view3"
            app:layout_constraintVertical_bias="0.0" />

        <View
            android:id="@+id/divider3"
            android:layout_width="2dp"
            android:layout_height="50dp"
            android:layout_below="@+id/city_res"
            android:layout_alignBottom="@+id/view3"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="10dp"
            android:layout_marginTop="-57dp"
            android:layout_marginEnd="138dp"
            android:layout_marginBottom="0dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="@+id/view3"
            app:layout_constraintVertical_bias="0.0" />

        <View
            android:id="@+id/divider2"
            android:layout_width="295dp"
            android:layout_height="2dp"
            android:layout_alignStart="@+id/name_res"
            android:layout_alignTop="@+id/view3"
            android:layout_alignEnd="@+id/view3"
            android:layout_marginStart="4dp"
            android:layout_marginTop="67dp"
            android:layout_marginEnd="0dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="@+id/view3"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/logo_img_res"
            android:layout_width="119dp"
            android:layout_height="106dp"
            android:layout_alignStart="@+id/view3"
            android:layout_alignBottom="@+id/divider"
            android:layout_marginStart="6dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="10dp"
            android:layout_toStartOf="@+id/divider"
            android:foregroundGravity="center_vertical|center_horizontal"
            android:visibility="visible"
            tools:srcCompat="@tools:sample/avatars" />

        <TextView
            android:id="@+id/name_res"
            android:layout_width="200dp"
            android:layout_height="41dp"
            android:layout_alignStart="@+id/divider"
            android:layout_alignTop="@+id/view3"
            android:layout_alignEnd="@+id/view3"
            android:layout_alignBottom="@+id/view3"
            android:layout_marginStart="-2dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="2dp"
            android:layout_marginBottom="91dp"
            android:gravity="center_horizontal|center_vertical"
            android:text="Yalla Yalla"
            android:textSize="25dp"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toTopOf="@+id/view3" />

        <TextView
            android:id="@+id/street_res"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/divider"
            android:layout_alignTop="@+id/view3"
            android:layout_alignBottom="@+id/view3"
            android:layout_marginStart="35dp"
            android:layout_marginTop="40dp"
            android:layout_marginBottom="63dp"
            android:gravity="center_horizontal|top"
            android:text="Struga"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toBottomOf="@+id/name_res" />

        <TextView
            android:id="@+id/apartment_number_res"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/name_res"
            android:layout_alignBottom="@+id/view3"
            android:layout_marginStart="2dp"
            android:layout_marginTop="37dp"
            android:layout_marginBottom="63dp"
            android:layout_toEndOf="@+id/street_res"
            android:gravity="center_horizontal|top"
            android:text="5"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toBottomOf="@+id/name_res" />

        <TextView
            android:id="@+id/min_price_res"
            android:layout_width="92sp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView6"
            android:layout_alignEnd="@+id/view3"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="160dp"
            android:gravity="center_horizontal|center_vertical"
            android:text="55,00 zł"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="92sp"
            android:layout_height="25dp"
            android:layout_alignTop="@+id/name_res"
            android:layout_alignEnd="@+id/view3"
            android:layout_marginTop="72dp"
            android:layout_marginEnd="160dp"
            android:gravity="center_horizontal"
            android:text="Min Price"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toBottomOf="@+id/street_res" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="93dp"
            android:layout_height="25dp"
            android:layout_alignTop="@+id/name_res"
            android:layout_alignEnd="@+id/view3"
            android:layout_marginStart="140dp"
            android:layout_marginTop="72dp"
            android:layout_marginEnd="25dp"
            android:gravity="center_horizontal"
            android:text="Delivery Cost"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toBottomOf="@+id/street_res" />

        <TextView
            android:id="@+id/delivert_cost_res"
            android:layout_width="92.5sp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView7"
            android:layout_alignEnd="@+id/view3"
            android:layout_marginStart="140dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="25dp"
            android:gravity="center_horizontal|center_vertical"
            android:text="7,00 zł"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/postocde_res"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/name_res"
            android:layout_alignBottom="@+id/view3"
            android:layout_marginStart="4dp"
            android:layout_marginTop="37dp"
            android:layout_marginBottom="63dp"
            android:layout_toEndOf="@id/apartment_number_res"
            android:gravity="center_horizontal|top"
            android:text="90-030"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toBottomOf="@+id/name_res" />

        <TextView
            android:id="@+id/city_res"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/name_res"
            android:layout_alignBottom="@+id/view3"
            android:layout_marginStart="2dp"
            android:layout_marginTop="37dp"
            android:layout_marginBottom="63dp"
            android:layout_toEndOf="@+id/postocde_res"
            android:gravity="center_horizontal|top"
            android:text="Lodz"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/divider"
            app:layout_constraintTop_toBottomOf="@+id/name_res" />

    </RelativeLayout>

</LinearLayout>